<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<div class="col-md-offset-1 col-md-4">
	<h2>Are you a Chef?</h2>
	<%= form_for( chef_session_path) do |f| %>
	    <div class="field">
	    	<%= f.label :email %><br />
	    	<%= f.email_field :email, autofocus: true %>
	    </div>

	    <div class="field">
	    	<%= f.label :password %><br />
	    	<%= f.password_field :password, autocomplete: "off" %>
	    </div>

	    <div class="actions">
	    	<%= f.submit "Log in" %>
	    </div>
	<% end %>
</div>
<div class="col-md-offset-2 col-md-4">
  	<h2>Are you looking for a Chef?</h2>
  	<%= form_for(user_session_path(@user)) do |f| %>
	    <div class="field">
	    	<%= f.label :email %><br />
	    	<%= f.email_field :email, autofocus: true %>
	    </div>

	    <div class="field">
	    	<%= f.label :password %><br />
	    	<%= f.password_field :password, autocomplete: "off" %>
	    </div>

	    <div class="actions">
	    	<%= f.submit "Log in" %>
	    </div>
	<% end %>
</div>
